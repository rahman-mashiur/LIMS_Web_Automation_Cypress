{
  "name": "lims_cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf -g ./cypress/reports/*.json",
    "test": "npm run cypress-test || npm run posttest",
    "cypress-test": "cypress run",
    "posttest": "npm run merge-report && npm run generate-htmlreport",
    "merge-report": "mochawesome-merge ./cypress/reports/*json > ./SingleReport.json",
    "generate-htmlreport": "marge ./SingleReport.json --reportDir ./cypress/reports"
  },
  "author": "Borshon",
  "license": "ISC",
  "dependencies": {
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress": "^13.14.2",
    "rimraf": "^6.0.1"
  }
}
